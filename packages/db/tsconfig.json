{
  // "extends": "../../tsconfig.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "noEmit": true,

    "paths": {
      "@/*": ["./src/*"]
    },

    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*", "drizzle.config.ts"],
  "exclude": ["node_modules"]
}
