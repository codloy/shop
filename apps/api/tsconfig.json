{
  // "extends": "../../tsconfig.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "noEmit": true,
    "skipLibCheck": true,

    "paths": {
      "@/*": ["./src/*"]
    },

    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["src/**/*", "../../packages/db"],
  "exclude": ["node_modules"]
}
